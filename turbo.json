{
  "npmClient": "yarn",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "migrate": {
      "dependsOn": [
        "^migrate"
      ],
      "cache": false
    }
  }
}
